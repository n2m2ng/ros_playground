<robot name="kyun" xmlns:xacro="http://playerstage.sourceforge.net/gazebo/xmlschema/#xacro">

<link name="world"/>
<joint name="world_to_base" type="fixed">
  <origin xyz="0 0 0"/>
  <parent link="world"/>
  <child link="base_link"/>
</joint>

<link name="base_link">
  <visual>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo01.stl" scale="0.001 0.001 0.001"/>
      <origin xyz="0 0 0"/>
    </geometry>
  </visual>
  <collision>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo01.stl" scale="0.001 0.001 0.001"/>
      <origin xyz="0 0 0"/>
    </geometry>
  </collision>
</link>

<gazebo reference="base_link">
  <material>Gazebo/Red</material>
</gazebo>

<joint name="base_to_2nd" type="continuous">
  <parent link="base_link"/>
  <child link="2nd_link"/>
  <origin xyz="0 0 0" rpy="0 0 0"/>
  <axis xyz="0 0 1"/>
</joint>

<link name="2nd_link">
  <visual>
    <geometry>
      <mesh filename="package://kyun_description/mesh/frame12.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </visual>
  <collision>
    <geometry>
      <mesh filename="package://kyun_description/mesh/frame12.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<gazebo reference="2nd_link">
  <material>Gazebo/Red</material>
</gazebo>

<joint name="2nd_to_3rd" type="continuous">
  <parent link="2nd_link"/>
  <child link="3rd_link"/>
  <origin xyz="-0.00963 0.019 0.05" rpy="0 0 0"/>
  <axis xyz="0 1 0"/>
  <dynamics damping="0.1"/>
</joint>

<link name="3rd_link">
  <visual>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo02.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </visual>
  <collision>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo02.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<gazebo reference="3rd_link">
  <material>Gazebo/Red</material>
</gazebo>

<joint name="3rd_to_4th" type="continuous">
  <parent link="3rd_link"/>
  <child link="4th_link"/>
  <origin xyz="0.019 -0.018 0.06075" rpy="0 0 0"/>
  <axis xyz="1 0 0"/>
  <dynamics damping="0.1"/>
</joint>

<link name="4th_link">
  <visual>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo03.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </visual>
  <collision>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo03.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<gazebo reference="4th_link">
  <material>Gazebo/Red</material>
</gazebo>

<joint name="4th_to_5th" type="continuous">
  <parent link="4th_link"/>
  <child link="5th_link"/>
  <origin xyz="-0.019 -0.02 0.024" rpy="0 0 0"/>
  <axis xyz="0 1 0"/>
  <dynamics damping="0.1"/>
</joint>

<link name="5th_link">
  <visual>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo04.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </visual>
  <collision>
    <geometry>
      <mesh filename="package://kyun_description/mesh/servo04.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<gazebo reference="5th_link">
  <material>Gazebo/Red</material>
</gazebo>

<joint name="5th_to_6th" type="continuous">
  <parent link="5th_link"/>
  <child link="6th_link"/>
  <origin xyz="0.0205 0.02 0.06075" rpy="0 0 0"/>
  <axis xyz="1 0 0"/>
  <dynamics damping="0.1"/>
</joint>

<link name="6th_link">
  <visual>
    <geometry>
      <mesh filename="package://kyun_description/mesh/frame13.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </visual>
  <collision>
    <geometry>
      <mesh filename="package://kyun_description/mesh/frame13.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<gazebo reference="6th_link">
  <material>Gazebo/Red</material>
</gazebo>

  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
      <robotNamespace>/kyun</robotNamespace>
      <robotSimType>gazebo_ros_control/DefaultRobotHWSim</robotSimType>
      <legacyModeNS>true</legacyModeNS>
    </plugin>
  </gazebo>
</robot>
